{
  "name": "@comparto/git-c",
  "description": "Conventional git cli.",
  "version": "1.0.1",
  "author": "@jimmy-guzman <jimmy.guzman@outlook.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "git-c": "./bin/run"
  },
  "bugs": "https://github.com/comparto/git-c/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/comparto/git-c",
  "repository": {
    "type": "git",
    "url": "https://github.com/comparto/git-c.git"
  },
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/ . --ext .ts",
    "lint:fix": "yarn lint --fix",
    "prettify": "prettier '**/**/*.{ts,md}' --write",
    "test": "jest",
    "test:ci": "yarn test",
    "test:coverage": "jest --coverage",
    "watch:test": "jest --watch",
    "watch:build": "tsc -w",
    "build": "run-s build:cli",
    "build:cli": "rm -rf lib && tsc -b",
    "build:readme": "oclif-dev readme --dir docs/ --multi && yarn prettify",
    "schema:build": "./scripts/build-schema.sh",
    "postpublish": "rm -f oclif.manifest.json",
    "release": "cross-env HUSKY_BYPASS=true npx semantic-release",
    "cz": "./bin/run c"
  },
  "gitc": {
    "useCommitlintConfig": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{md}": [
      "prettier --write"
    ],
    "*.{ts}": [
      "eslint --fix"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "git-c",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "prettier": "@comparto/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^3.1.0",
    "ajv": "^6.12.6",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "tslib": "^1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@comparto/eslint-config": "^5.0.2",
    "@comparto/prettier-config": "^1.0.0",
    "@oclif/dev-cli": "^1.22.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/inquirer": "^7.3.0",
    "@types/jest": "^26.0.14",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^10",
    "commitlint": "^11.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "globby": "^10",
    "husky": "^4.2.5",
    "jest": "^26.5.3",
    "lint-staged": "^10.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.8",
    "ts-jest": "^26.4.1",
    "ts-json-schema-generator": "^0.76.1",
    "ts-node": "^8",
    "tscpaths": "^0.0.9",
    "typescript": "^3.9.5"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/lib"
  ]
}
